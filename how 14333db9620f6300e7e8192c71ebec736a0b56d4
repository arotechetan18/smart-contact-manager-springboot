[32m+[m[32m  const contactModal = new Modal(viewContactModal, options, instanceOptions);[m
[32m+[m
[32m+[m[32m  window.loadContactdata = async function (id) {[m
[32m+[m[32m    try {[m
[32m+[m[32m      console.log("Loading ID:", id);[m
[32m+[m
[32m+[m[32m      const response = await fetch(`/api/contacts/${id}`);[m
[32m+[m
[32m+[m[32m      if (!response.ok) {[m
[32m+[m[32m        throw new Error("Failed to fetch contact");[m
[32m+[m[32m      }[m
[32m+[m
[32m+[m[32m      const data = await response.json();[m
[32m+[m[32m      console.log(data);[m
[32m+[m
[32m+[m[41m      [m
[32m+[m[32m      document.getElementById("Contact_Name").innerText = data.name || "";[m
[32m+[m
[32m+[m[41m     [m
[32m+[m[32m      document.getElementById("Contact_Email").innerText = data.email || "";[m
[32m+[m
[32m+[m[41m     [m
[32m+[m[32m      document.getElementById("Contact_phoneNumber").innerText =[m
[32m+[m[32m        data.phoneNumber || "";[m
[32m+[m[41m    [m
[32m+[m[32m      document.getElementById("Contact_Address").innerText =[m
[32m+[m[32m        data.address || "Not Available";[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m      document.getElementById("Contact_Description").innerText =[m
[32m+[m[32m        data.description || "No Description";[m
[32m+[m
[32m+[m[41m [m
[32m+[m[32m      document.getElementById("Contact_Image").src =[m
[32m+[m[32m        data.picture && data.picture.trim() !== ""[m
[32m+[m[32m          ? data.picture[m
[32m+[m[32m          : "https://via.placeholder.com/150";[m
[32m+[m
[32m+[m[32m      document.getElementById("Contact_Website").href =[m
[32m+[m[32m        data.websiteLink && data.websiteLink.trim() !== ""[m
[32m+[m[32m          ? data.websiteLink[m
[32m+[m[32m          : "#";[m
[32m+[m
[32m+[m[41m     [m
[32m+[m[32m      document.getElementById("Contact_Linkedin").href =[m
[32m+[m[32m        data.linkdInLink && data.linkdInLink.trim() !== ""[m
[32m+[m[32m          ? data.linkdInLink[m
[32m+[m[32m          : "#";[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m      const fav = document.getElementById("Contact_Favourite");[m
